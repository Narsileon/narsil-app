{
	"name": "narsil/app",
	"description": "Narsil App",
	"type": "project",
	"license": "MIT",
	"keywords": [
		"app",
		"inertia",
		"laravel",
		"narsil",
		"php",
		"react"
	],
	"authors": [
		{
			"name": "Jonathan Rigaux"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-auth.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-calendar.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-contacts.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-forms.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-legals.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-localization.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-mails.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-menus.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-policies.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-settings.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-storage.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-table.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-tree.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-ui.git"
		}
	],
	"require": {
		"php": "^8.2",
		"inertiajs/inertia-laravel": "^1.3",
		"laravel/framework": "^11.3",
		"narsil/auth": "dev-main",
		"narsil/calendar": "dev-main",
		"narsil/contacts": "dev-main",
		"narsil/forms": "dev-main",
		"narsil/legals": "dev-main",
		"narsil/localization": "dev-main",
		"narsil/mails": "dev-main",
		"narsil/menus": "dev-main",
		"narsil/policies": "dev-main",
		"narsil/settings": "dev-main",
		"narsil/storage": "dev-main",
		"narsil/table": "dev-main",
		"narsil/tree": "dev-main",
		"narsil/ui": "dev-main",
		"tightenco/ziggy": "^2.1"
	},
	"require-dev": {
		"fakerphp/faker": "^1.23",
		"laravel/pint": "^1.13",
		"laravel/sail": "^1.26",
		"mockery/mockery": "^1.6",
		"nunomaduro/collision": "^8.0",
		"phpunit/phpunit": "^11.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	}
}
